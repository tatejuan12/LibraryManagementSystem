<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label text="Add a Member below" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
    <TextField id="memberName" fx:id="memberName" promptText="Member Name" />
    <TextField id="memberPhone" fx:id="memberPhone" layoutX="120.0" layoutY="64.0" promptText="Member Phone Number" />
    <Button fx:id="addMemberButton" text="Add Member" onAction="#addMemberController" />
    <Button fx:id="backToMainSceneButton" text="Back to Dashboard" onAction="#backToMainSceneController" />
    <Label fx:id="errorField" text="" style="-fx-font-size: 15px; -fx-text-fill: red" />
  </children>
  <padding>
    <Insets bottom="20.0" left="250.0" right="250.0" top="20.0" />
  </padding>
</VBox>